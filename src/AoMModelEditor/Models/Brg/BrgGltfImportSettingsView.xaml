<reactiveui:ReactiveUserControl x:Class="AoMModelEditor.Models.Brg.BrgGltfImportSettingsView"
                                x:TypeArguments="local:BrgGltfImportSettingsViewModel"
                                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                                xmlns:local="clr-namespace:AoMModelEditor.Models.Brg"
                                xmlns:reactiveui="http://reactiveui.net"
                                mc:Ignorable="d"
                                d:DesignHeight="450" d:DesignWidth="800">
    <GroupBox x:Name="mainGroupBox">
        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
            <StackPanel>
                <StackPanel Orientation="Horizontal" Margin="0 0 0 5">
                    <Label Content="Sample Rate (FPS)" Margin="0 0 5 0" />
                    <TextBox x:Name="sampleRateTextBox" Width="40" Margin="0 0 5 0" VerticalContentAlignment="Center" />
                </StackPanel>
            </StackPanel>
        </ScrollViewer>
    </GroupBox>
</reactiveui:ReactiveUserControl>
